<%- include('../../includes/top.ejs') %>
  <!-- Page Title -->
  <h1 class="h2 mt-5 mb-3">Reserve the car</h1>
  <!-- Car Add-Form -->
  <form action="/parking/add-car" method="POST">
    <div class="mb-3">
      <input type="hidden" name="parkingName" value="<%= parkingName %>">
      <label for="carNum" class="form-label">Car Number</label>
      <select class="form-select" name="carNum">
        <option value="select-your-car" selected>Select your car</option>
        <% for (car of cars) { %>
          <option value="<%= car.carNum %>"><%= car.carNum %></option>
        <% } %>
      </select>
    </div>
    <div class="d-flex mt-4">
      <a href="/parking/overview" class="btn btn-secondary"><i class="bi bi-arrow-left"></i>&nbsp;Back</a>
      &nbsp;
      <button type="submit" class="btn btn-primary"><i class="bi bi-check-lg"></i>&nbsp;Submit</button>
    </div>
  </form>
<%- include('../../includes/bottom.ejs') %>
